#_______________________________________________________________________
# RUN CONTROL SECTION

  RUN_NAME = 'STC00'
  DESCRIPTION = 'Species transport with cyclic BCs'

  RUN_TYPE = 'NEW'
  UNITS = 'CGS'

  TSTOP = 1.0

  DT = 1.0d-4

  MOMENTUM_X_EQ= .FALSE.    .FALSE.
  MOMENTUM_Y_EQ= .FALSE.    .FALSE.
  MOMENTUM_Z_EQ= .FALSE.    .FALSE.

  ENERGY_EQ  = .FALSE.
  SPECIES_EQ = .TRUE.    .FALSE.



#_______________________________________________________________________
# GEOMETRY SECTION

  COORDINATES = 'CARTESIAN'

  XLENGTH  =   7.0   IMAX =  10 !   16
  ZLENGTH  =  20.0   KMAX =  20 !   16
  YLENGTH  = 100.0   JMAX =  10 ! 1024

  CYCLIC_X = .T.
  CYCLIC_Z = .T.

#_______________________________________________________________________
# MATERIAL SECTION

  RO_g0 = 1.2E-3
  MU_g0 = 1.8E-4
  Dif_g0 = 0.0

! Gas Section
!---------------------------------------------------------------------//
  MU_g0 = 1.663E-4  ! Constant Viscosity

  NMAX_g = 2

  SPECIES_g(1) = 'AIR'  SPECIES_ALIAS_g(1) = 'AIR'
  SPECIES_g(2) = 'AIR'  SPECIES_ALIAS_g(2) = 'AIR2'


! Particle Section:
!---------------------------------------------------------------------//
  e =        0.8     ! part-part restitution coefficient
  Phi =      0.0     ! angle of internal friction

  EP_star =  0.42    ! void fraction at maximum packing
  RDF_TYPE = 'CARNAHAN_STARLING'  !rad. dist. fun. at contact        

! Solids phase 1
!---------------------------------------------------------------------//
  RO_s0 =   2.00
  D_p0 =    0.04


#_______________________________________________________________________
# INITIAL CONDITIONS SECTION

!                 Bed           Freeboard
  IC_X_w =        0.0             0.0
  IC_X_e =        7.0             7.0
  IC_Y_s =        0.0            50.0
  IC_Y_n =       50.0           100.0
  IC_Z_b =        0.0             0.0
  IC_Z_t =       20.0            20.0

  IC_EP_g =       0.42            1.0

  IC_U_g =       40.0            40.0
  IC_V_g =   @(25.9/0.42)        25.9
  IC_W_g =       40.0            40.0

  IC_U_s(1,1) =   0.0             0.0
  IC_V_s(1,1) =   0.0             0.0
  IC_W_s(1,1) =   0.0             0.0

  IC_X_g(1,1) =   1.0             1.0

  IC_P_star =     0.0             0.0
  IC_T_g =      300.0           300.0

! South-west corner with species 2
  IC_X_w(5) =     0.0
  IC_X_e(5) =     2.0
  IC_Y_s(5) =     0.0
  IC_Y_n(5) =    10.0
  IC_Z_b(5) =     0.0
  IC_Z_t(5) =     2.0

  IC_X_g(5,1) =   0.0
  IC_X_g(5,2) =   1.0

  IC_TYPE(5) = 'PATCH'

#_______________________________________________________________________
# BOUNDARY CONDITIONS SECTION
!                Jet   Grid     Exit
  BC_X_w =        0.0    1.0      0.0
  BC_X_e =        1.0    7.0      7.0
  BC_Y_s =        0.0    0.0    100.0
  BC_Y_n =        0.0    0.0    100.0
  BC_Z_b =        0.0    0.0      0.0
  BC_Z_t =       20.0   20.0     20.0

  BC_TYPE =      'MI'   'MI'    'PO'

  BC_EP_g =       1.0    1.0

  BC_U_g =        0.0    0.0
  BC_V_g =       25.9   25.9
  BC_W_g =        0.0    0.0
  BC_X_g(1,1) =   0.0    1.0
  BC_X_g(1,2) =   1.0    0.0

  BC_P_g =      3*1.013d6
  BC_T_g =    3*300.0

#_______________________________________________________________________
# OUTPUT CONTROL SECTION

  RES_DT = 0.05

  SPX_DT(1) = 0.05   ! EP_g
  SPX_DT(2) = 0.10   ! P_g, P_star
  SPX_DT(3) = 0.10   ! U_g, V_g, W_g
  SPX_DT(4) = 0.10   ! U_s, V_s, W_s
  SPX_DT(5) = 100.   ! ROP_s
  SPX_DT(6) = 100.   ! T_g, T_s
  SPX_DT(7) = 0.05   ! X_g, X_s
  SPX_DT(8) = 100.   ! theta
  SPX_DT(9) = 100.   ! Scalar

  RESID_STRING = 'P0' 'X001', 'X002'

  NLOG = 100
  FULL_LOG = .T.


#_______________________________________________________________________
# DMP SETUP

  NODESI =  2  NODESJ =  1  NODESK =  2

  ENABLE_DMP_LOG = .F.
  CHK_BATCHQ_END = .F.
