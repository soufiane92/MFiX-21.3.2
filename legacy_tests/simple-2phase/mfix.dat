#
#  Simple two-phase flow
#
#
# Run-control section
#
  RUN_NAME = 'PH2'
  DESCRIPTION = 'Simple two-phase flow'
  RUN_TYPE = 'new'
  UNITS = 'cgs'
  TSTOP = 0.3
  DT = 1.0E-4
