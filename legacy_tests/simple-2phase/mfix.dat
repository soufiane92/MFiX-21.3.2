  ur_fac(1) = 0.5                          !pressure

  ur_fac(3) = 0.2                          !u-velocity

  ur_fac(4) = 0.2                          !v-velocity

#
# Gas-phase Section
#
  RO_g0 = 1.2E-3
  MU_g0 = 1.8E-4
