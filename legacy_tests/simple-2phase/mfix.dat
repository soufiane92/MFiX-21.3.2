  IC_EP_g          =  .9
  IC_P_g           = 0.0

  IC_U_g           =   0.0
  IC_V_g           =   0.0

  IC_U_s(1,1)      =  0.0
  IC_V_s(1,1)      =  0.0

#
#  Boundary Conditions Section
#
       !               Exit
  BC_X_w           =  0.0
  BC_X_e           =  5.0
  BC_Y_s           =  5.0
  BC_Y_n           =  5.0

  BC_TYPE          =  'PO'

  BC_P_g           = 0.0

#
#  Output Control
#
  RES_DT = 0.01
  OUT_DT = 10.
        !
        ! EP_g P_g       U_g  U_s  ROP_s     T_g  X_g
        !      P_star    V_g  V_s            T_s1 X_s    Theta
        !                W_g  W_s            T_s2
  SPX_DT = 0.01 0.1      0.1  0.1  100.      100. 100.   100.   100.

  FULL_LOG = .T.
  NLOG   = 100
  TOL_DIVERGE = 1.0D+6

